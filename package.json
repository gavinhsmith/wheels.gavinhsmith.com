{
  "name": "wheels.gavinhsmith.com",
  "packageManager": "yarn@4.4.1",
  "private": true,
  "devDependencies": {
    "express": "^4.21.2",
    "handlebars": "^4.7.8",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.17"
  },
  "scripts": {
    "build": "rimraf ./dist && node ./build.js",
    "start": "serve ./dist",
    "dev:css": "tailwindcss -i ./src/tailwind.css -o ./dist/css/main.css --watch",
    "dev:live": "node ./dev.js",
    "dev": "npm-run-all --parallel dev:css dev:live --print-label"
  }
}
